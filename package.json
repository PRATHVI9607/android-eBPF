{
  "name": "android-ebpf-profiler",
  "version": "1.0.0",
  "description": "Android eBPF Profiler - Kernel tracing automation system",
  "main": "backend/app.py",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "jest --testPathPattern=backend",
    "test:frontend": "jest --testPathPattern=frontend",
    "start": "bash start.sh",
    "dev:backend": "cd backend && python3 app.py",
    "dev:frontend": "cd frontend && python3 -m http.server 8000"
  },
  "keywords": [
    "ebpf",
    "bpftrace",
    "android",
    "profiling",
    "tracing",
    "kernel"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  }
}
