# ============================================================================
# Android eBPF Profiler Configuration
# ============================================================================

# Backend Settings
BACKEND_HOST=0.0.0.0
BACKEND_PORT=5000
BACKEND_DEBUG=True

# Frontend Settings
FRONTEND_HOST=localhost
FRONTEND_PORT=8000

# Output Settings
OUTPUT_DIR=./output
MAX_TRACE_DURATION=300
DEFAULT_TRACE_DURATION=30

# ADB Settings
ADB_TIMEOUT=30
ADB_DEVICE_TIMEOUT=10

# Trace Settings
JSON_OUTPUT=True
NDJSON_FORMAT=True

# API Settings
CORS_ORIGIN=*
API_TIMEOUT=600

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
